var camelizeString = function camelizeString(string) {
    return (string || '').replace(/_([a-z])/g, function (substring) {
        return substring[1].toUpperCase();
    });
};

var decamelizeString = function decamelizeString(string) {
    var result = (string || '').replace(/([A-Z])/g, function (substring) {
        return '_' + substring[0].toLocaleLowerCase();
    }).replace(/^_/, '');

    return result;
};

